    </xs:complexType>
    <!-- Pricing & Promo -->
    <xs:complexType name="GrpWarningsType">
        <xs:annotation>
            <xs:documentation>Warning detail for Groups Messages</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="Warning" minOccurs="0" maxOccurs="99">
                <xs:complexType>
                    <xs:simpleContent>
                        <xs:extension base="GrpWarningType"/>
                    </xs:simpleContent>
                </xs:complexType>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="GrpWarningType">
        <xs:annotation>
            <xs:documentation xml:lang="en">Standard way to indicate successful processing of an OTA message, but one in
                which warnings are generated
            </xs:documentation>
        </xs:annotation>
        <xs:simpleContent>
            <xs:extension base="FreeTextType">
                <xs:attribute name="Type" type="OTA_CodeType" use="required">
                    <xs:annotation>
                        <xs:documentation xml:lang="en">The Warning element MUST contain the Type attribute that uses a
                            recommended set of values to indicate the warning type. The validating XSD can expect to
                            accept values that it has NOT been explicitly coded for and process them by using Type
                            ="Unknown". Refer to OTA Code List Error Warning Type (EWT).
                        </xs:documentation>
                    </xs:annotation>
                </xs:attribute>
                <xs:attributeGroup ref="GrpErrorWarningAttributeGroup"/>
            </xs:extension>
        </xs:simpleContent>
    </xs:complexType>
    <xs:complexType name="AdditionalAgentInfoType">
        <xs:annotation>
            <xs:documentation>Specifies the information of the an agent who is creating or modifying the booking,
                information beyond POS or mosttimes the same as POS information coming across.
            </xs:documentation>
        </xs:annotation>
