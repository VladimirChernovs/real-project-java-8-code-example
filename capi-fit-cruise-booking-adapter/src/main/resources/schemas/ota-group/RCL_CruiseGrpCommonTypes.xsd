        <xs:attribute name="SingleOccupancyQty" type="Numeric0to999" use="optional">
            <xs:annotation>
                <xs:documentation xml:lang="en">Single Occupancy Seats Requested</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="DoubleOccupancyQty" type="Numeric0to999" use="optional">
            <xs:annotation>
                <xs:documentation xml:lang="en">Double Occupancy Seats Requested</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="TripleOccupancyQty" type="Numeric0to999" use="optional">
            <xs:annotation>
                <xs:documentation xml:lang="en">Triple Occupancy Seats Requested</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="QuadOccupancyQty" type="Numeric0to999" use="optional">
            <xs:annotation>
                <xs:documentation xml:lang="en">Quad Occupancy Seats Requested</xs:documentation>
            </xs:annotation>
        </xs:attribute>
