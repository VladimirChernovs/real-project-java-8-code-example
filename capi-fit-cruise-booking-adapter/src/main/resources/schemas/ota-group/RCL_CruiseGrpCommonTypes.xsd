                        </xs:complexType>
                    </xs:element>
                    <xs:element name="Transportation" type="GuestTransportationType" minOccurs="0" maxOccurs="5">
                        <xs:annotation>
                            <xs:documentation xml:lang="en">Specifies the mode of the transportation from the guest
                                location to the cruise port of departure.
                            </xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="Information" type="ParagraphType" minOccurs="0" maxOccurs="5">
                        <xs:annotation>
                            <xs:documentation xml:lang="en">Specifies cruise line registration procedures and other free
                                text information. The name attribute is used to define the type of information.
                                Registration procedures are messages that advise customers who are not registered with
                                the cruise line how to do so. Usually non registered customers cannot get access to the
                                cruise line inventory.
                            </xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="GroupPolicyInfo" minOccurs="0" maxOccurs="10">
                        <xs:annotation>
                            <xs:documentation xml:lang="en">Identifies the availability of the particular sitting.
                            </xs:documentation>
                        </xs:annotation>
                        <xs:complexType>
                            <xs:attributeGroup ref="GroupPolicyTypeGroup"/>
                        </xs:complexType>
                    </xs:element>
                    <xs:element name="CategoryOption" type="CategoryOptionType" maxOccurs="6"/>
                </xs:sequence>
                <xs:attribute name="MaxCabinOccupancy" type="Numeric1to99" use="optional">
                    <xs:annotation>
                        <xs:documentation xml:lang="en">Maximum cabin occupancy available on this specific sailing
                            (ship).
                        </xs:documentation>
                    </xs:annotation>
                </xs:attribute>
                <xs:attribute name="CategoryLocation" type="CategoryLocationType" use="optional">
                    <xs:annotation>
                        <xs:documentation xml:lang="en">Specifies the different category locations available on the
                            ship.
                        </xs:documentation>
                    </xs:annotation>
                </xs:attribute>
                <xs:attribute name="GAPPoints" type="Numeric0to999" use="optional">
                    <xs:annotation>
                        <xs:documentation>Provides redemption points for group amenities</xs:documentation>
                    </xs:annotation>
                </xs:attribute>
                <xs:attribute name="HasGroupsInd" type="YesNoType" use="optional">
                    <xs:annotation>
                        <xs:documentation>Indicates if the Agent has group on this sailing.</xs:documentation>
                    </xs:annotation>
                </xs:attribute>
                <xs:attribute name="TPA_Status" type="StringLength0to32">
                    <xs:annotation>
                        <xs:documentation>Specifies the status of the returned sailing.</xs:documentation>
                    </xs:annotation>
                </xs:attribute>
                <xs:attribute name="NextSailingDate" type="DateOrDateTimeType" use="optional">
                    <xs:annotation>
                        <xs:documentation>Indicates the Sailing date after the returned Sailing.</xs:documentation>
                    </xs:annotation>
                </xs:attribute>
                <xs:attribute name="PreviousSailingDate" type="DateOrDateTimeType" use="optional">
                    <xs:annotation>
                        <xs:documentation>Indicates the Sailing date before the returned Sailing.</xs:documentation>
                    </xs:annotation>
                </xs:attribute>
                <xs:attribute name="StandardPPGRAmount" type="Money">
                    <xs:annotation>
                        <xs:documentation>The pre-paid gratuities amount for a standard cabin per Sailing.
                        </xs:documentation>
                    </xs:annotation>
                </xs:attribute>
                <xs:attribute name="SpecialtyPPGRAmount" type="Money">
                    <xs:annotation>
                        <xs:documentation>The pre-paid gratuities amount for a specialty cabin per Sailing.
                        </xs:documentation>
                    </xs:annotation>
                </xs:attribute>
                <xs:attribute name="MinimumCabinsRequired" type="Numeric1to99" use="optional">
                    <xs:annotation>
                        <xs:documentation xml:lang="en">Minimum number of Cabins required in this in this category to
                            create a group
                        </xs:documentation>
                    </xs:annotation>
                </xs:attribute>
                <xs:attribute name="MaximumCabinsAllowed" type="Numeric1to99" use="optional">
                    <xs:annotation>
                        <xs:documentation xml:lang="en">Maximum number of Cabins allowed in this category for the
                            group
                        </xs:documentation>
                    </xs:annotation>
                </xs:attribute>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
    <xs:complexType name="GrpSelectedCategoryType">
        <xs:annotation>
            <xs:documentation>Specifies Selected category information for a reservation, includes cabin selection and
                cabin attribute details
            </xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="SelectedPromotions" type="TPA_PromotionInfoType" minOccurs="0" maxOccurs="25"/>
            <xs:element name="CabinAttributes" minOccurs="0">
                <xs:annotation>
                    <xs:documentation xml:lang="en">Collection of category-level cabin attributes.</xs:documentation>
                </xs:annotation>
                <xs:complexType>
                    <xs:sequence>
                        <xs:element name="CabinAttribute" maxOccurs="99">
                            <xs:annotation>
                                <xs:documentation>List of attributes associated with all cabins in the specified
                                    category.
                                </xs:documentation>
                            </xs:annotation>
                            <xs:complexType>
                                <xs:attribute name="CabinAttributeCode" type="OTA_CodeType" use="optional">
                                    <xs:annotation>
                                        <xs:documentation xml:lang="en">Specifies the type of cabin requested. Refer to
                                            OTA Code List Cabin Filters (CBF).
                                        </xs:documentation>
                                    </xs:annotation>
                                </xs:attribute>
                            </xs:complexType>
                        </xs:element>
                    </xs:sequence>
                </xs:complexType>
            </xs:element>
            <xs:element name="SelectedCabins" minOccurs="0">
                <xs:complexType>
                    <xs:sequence>
                        <xs:element name="SelectedCabin" minOccurs="0" maxOccurs="unbounded">
                            <xs:annotation>
                                <xs:documentation xml:lang="en">Specifies the selected cabin information.
                                </xs:documentation>
                            </xs:annotation>
                            <xs:complexType>
                                <xs:complexContent>
                                    <xs:extension base="CabinOptionType">
                                        <xs:sequence>
                                            <xs:element name="CabinAttributes" minOccurs="0">
                                                <xs:annotation>
                                                    <xs:documentation>Collection of cabin attributes.</xs:documentation>
                                                </xs:annotation>
                                                <xs:complexType>
                                                    <xs:sequence>
                                                        <xs:element name="CabinAttribute" maxOccurs="99">
                                                            <xs:annotation>
                                                                <xs:documentation>List of attributes associated with
                                                                    this cabin.
                                                                </xs:documentation>
                                                            </xs:annotation>
                                                            <xs:complexType>
                                                                <xs:attribute name="CabinAttributeCode"
                                                                              type="OTA_CodeType" use="optional">
                                                                    <xs:annotation>
                                                                        <xs:documentation xml:lang="en">Specifies the
                                                                            type of cabin requested. Refer to OTA Code
                                                                            List Cabin Filters (CBF).
                                                                        </xs:documentation>
                                                                    </xs:annotation>
                                                                </xs:attribute>
                                                            </xs:complexType>
                                                        </xs:element>
                                                    </xs:sequence>
                                                </xs:complexType>
                                            </xs:element>
                                        </xs:sequence>
                                        <xs:attribute name="RetainCabinFlag" type="YesNoType" use="optional">
                                            <xs:annotation>
                                                <xs:documentation>If Y , it indicates that the cabin is not released to
                                                    the general inventory even if its released from a booking. The cabin
                                                    will remain at the group allocation level.
                                                </xs:documentation>
                                            </xs:annotation>
                                        </xs:attribute>
                                        <xs:attribute name="CabinInventoryActionCode" type="AlphaNumericStringLength1"
                                                      use="optional">
                                            <xs:annotation>
                                                <xs:documentation>Indicates the action performed on the selected cabin.
                                                    'A' - Allocate
                                                    'D' - De-Allocate
                                                    'R' - Release/Cancel
                                                    'N' - No Action
                                                    ' ' - No Action ( Blank )
                                                </xs:documentation>
                                            </xs:annotation>
                                        </xs:attribute>
                                    </xs:extension>
                                </xs:complexContent>
                            </xs:complexType>
                        </xs:element>
                    </xs:sequence>
                </xs:complexType>
            </xs:element>
        </xs:sequence>
        <xs:attributeGroup ref="CruiseCategoryDetailGroup">
