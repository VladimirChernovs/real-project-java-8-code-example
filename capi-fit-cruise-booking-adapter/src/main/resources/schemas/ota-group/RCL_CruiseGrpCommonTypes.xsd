            <xs:annotation>
                <xs:documentation xml:lang="en">This flag indicates whether this particulal dining type and seating can
                    be cross-referenced with others.
                </xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="FamilyTimeIndicator" type="xs:boolean" use="optional">
            <xs:annotation>
                <xs:documentation xml:lang="en">This flag indicates whether this dining seating supports Family time
                </xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="PrepaidGratuityRequired" type="xs:boolean" use="optional">
            <xs:annotation>
                <xs:documentation xml:lang="en">Indicates whether prepaid gratuities are required for this particular
                    dining type ans seating
                </xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="SittingType" use="optional">
            <xs:annotation>
                <xs:documentation xml:lang="en">Dining sitting type such as Traditional ( for Main and Second), Open,
                    etc
