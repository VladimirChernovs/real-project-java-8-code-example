        </xs:attribute>
        <xs:attribute name="SittingInstance" type="StringLength1to16" use="optional">
            <xs:annotation>
                <xs:documentation xml:lang="en">Dining sitting Time/Instance Information</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="DiningConfiguration" use="optional">
            <xs:annotation>
                <xs:documentation>Indicates Dining configuration for the whole group. Nametoken.</xs:documentation>
            </xs:annotation>
