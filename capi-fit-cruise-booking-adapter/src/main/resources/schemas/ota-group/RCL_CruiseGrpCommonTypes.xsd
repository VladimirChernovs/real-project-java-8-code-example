                <xs:documentation xml:lang="en">Agent or agency's identifier.</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="AgencyFaxNumber" type="StringLength1to32" use="optional">
            <xs:annotation>
                <xs:documentation xml:lang="en">Fax number for Agency recorded in the booking</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="SecondAgencyID" type="StringLength1to32" use="optional">
            <xs:annotation>
                <xs:documentation xml:lang="en">Agent or agency's identifier **** Further definition Groups SME*****.
                </xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="SecondAgencyPhoneNumber" type="StringLength1to32" use="optional">
            <xs:annotation>
                <xs:documentation xml:lang="en">Telephone number for Second Agency recorded in the booking
                </xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="AgencyTravelCounsellor" type="StringLength1to32" use="optional">
            <xs:annotation>
                <xs:documentation xml:lang="en">Agency Travel Counsellor</xs:documentation>
            </xs:annotation>
        </xs:attribute>
