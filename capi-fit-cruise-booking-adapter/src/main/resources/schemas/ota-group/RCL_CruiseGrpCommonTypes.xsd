    <xs:complexType name="SelectedCategoryType">
        <xs:annotation>
            <xs:documentation>Specifies Selected category information for a reservation, includes cabin selection and
                cabin attribute details
            </xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="SelectedPromotions" type="TPA_PromotionInfoType" minOccurs="0" maxOccurs="25"/>
            <xs:element name="CabinAttributes" minOccurs="0">
                <xs:annotation>
                    <xs:documentation xml:lang="en">Collection of category-level cabin attributes.</xs:documentation>
                </xs:annotation>
                <xs:complexType>
                    <xs:sequence>
                        <xs:element name="CabinAttribute" maxOccurs="99">
                            <xs:annotation>
                                <xs:documentation>List of attributes associated with all cabins in the specified
                                    category.
                                </xs:documentation>
                            </xs:annotation>
                            <xs:complexType>
                                <xs:attribute name="CabinAttributeCode" type="OTA_CodeType" use="optional">
                                    <xs:annotation>
                                        <xs:documentation xml:lang="en">Specifies the type of cabin requested. Refer to
                                            OTA Code List Cabin Filters (CBF).
                                        </xs:documentation>
                                    </xs:annotation>
                                </xs:attribute>
                            </xs:complexType>
                        </xs:element>
                    </xs:sequence>
                </xs:complexType>
            </xs:element>
            <xs:element name="SelectedCabins" minOccurs="0">
                <xs:complexType>
                    <xs:sequence>
                        <xs:element name="SelectedCabin" minOccurs="0" maxOccurs="4">
                            <xs:annotation>
                                <xs:documentation xml:lang="en">Specifies the selected cabin information.
                                </xs:documentation>
                            </xs:annotation>
                            <xs:complexType>
                                <xs:complexContent>
                                    <xs:extension base="CabinOptionType">
                                        <xs:sequence>
                                            <xs:element name="CabinAttributes" minOccurs="0">
                                                <xs:annotation>
                                                    <xs:documentation>Collection of cabin attributes.</xs:documentation>
                                                </xs:annotation>
                                                <xs:complexType>
                                                    <xs:sequence>
                                                        <xs:element name="CabinAttribute" maxOccurs="99">
                                                            <xs:annotation>
                                                                <xs:documentation>List of attributes associated with
                                                                    this cabin.
                                                                </xs:documentation>
                                                            </xs:annotation>
                                                            <xs:complexType>
                                                                <xs:attribute name="CabinAttributeCode"
                                                                              type="OTA_CodeType" use="optional">
                                                                    <xs:annotation>
                                                                        <xs:documentation xml:lang="en">Specifies the
                                                                            type of cabin requested. Refer to OTA Code
                                                                            List Cabin Filters (CBF).
                                                                        </xs:documentation>
                                                                    </xs:annotation>
                                                                </xs:attribute>
                                                            </xs:complexType>
                                                        </xs:element>
                                                    </xs:sequence>
                                                </xs:complexType>
                                            </xs:element>
                                        </xs:sequence>
                                        <xs:attribute name="RetainCabinFlag" type="YesNoType" use="optional">
                                            <xs:annotation>
                                                <xs:documentation>If Y , it indicates that the cabin is not released to
                                                    the general inventory even if its released from a booking. The cabin
                                                    will remain at the group allocation level.
                                                </xs:documentation>
                                            </xs:annotation>
                                        </xs:attribute>
                                        <xs:attribute name="CabinInventoryActionCode" type="AlphaNumericStringLength1"
                                                      use="optional">
                                            <xs:annotation>
                                                <xs:documentation>Indicates the action performed on the selected cabin.
                                                    'A' - Allocate
                                                    'D' - De-Allocate
                                                    'R' - Release/Cancel
                                                    'N' - No Action
                                                    ' ' - No Action ( Blank )
                                                </xs:documentation>
                                            </xs:annotation>
                                        </xs:attribute>
                                    </xs:extension>
                                </xs:complexContent>
                            </xs:complexType>
                        </xs:element>
                    </xs:sequence>
                </xs:complexType>
            </xs:element>
        </xs:sequence>
        <xs:attributeGroup ref="CruiseCategoryDetailGroup">
