            <xs:extension base="SailingInfoType">
                <xs:sequence>
                    <xs:element name="Dining" minOccurs="0" maxOccurs="10">
                        <xs:annotation>
                            <xs:documentation xml:lang="en">Identifies the availability of the particular sitting.
                            </xs:documentation>
                        </xs:annotation>
                        <xs:complexType>
                            <xs:attribute name="Sitting" type="StringLength1to64" use="required">
                                <xs:annotation>
                                    <xs:documentation xml:lang="en">Dining service. Typical values are: first, second,
                                        third, open.
                                    </xs:documentation>
                                </xs:annotation>
                            </xs:attribute>
                            <xs:attribute name="Status" type="OTA_CodeType" use="optional">
                                <xs:annotation>
                                    <xs:documentation xml:lang="en">Indicates the dining status. Refer to OTA Code table
                                        Status (STS).
                                    </xs:documentation>
                                </xs:annotation>
