                <xs:restriction base="xs:NMTOKEN">
                    <xs:enumeration value="Apart"/>
                    <xs:enumeration value="Together"/>
                    <xs:enumeration value="RoundRobin"/>
                    <xs:enumeration value="DoNotSeat"/>
                </xs:restriction>
            </xs:simpleType>
        </xs:attribute>
        <xs:attribute name="RuleId" type="StringLength1to16" use="optional">
            <xs:annotation>
                <xs:documentation xml:lang="en">Dining sitting - Specialty Inventory/ Dining Profile Identification
                </xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="AvailableQuantity" type="Numeric0to9999" use="optional">
            <xs:annotation>
                <xs:documentation xml:lang="en">Specifies the quantity of the Dining Availability</xs:documentation>
            </xs:annotation>
        </xs:attribute>
    </xs:attributeGroup>
    <xs:complexType name="TPA_DiningProfileType">
        <xs:annotation>
            <xs:documentation xml:lang="en">Used to specify the Specialty Dining rules and attributes</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="DiningProfile" minOccurs="0" maxOccurs="unbounded">
                <xs:annotation>
                    <xs:documentation xml:lang="en">
                        Contains the cruise attribute information, identifying those items which are mandatory,
                        modifiable, or have required maximums.
                    </xs:documentation>
                </xs:annotation>
                <xs:complexType>
                    <xs:attribute name="Code" use="optional">
                        <xs:annotation>
                            <xs:documentation xml:lang="en">Identifies the cruise options for which parameters are being
                                set. Can be a Category Code, Loyalty group code etc.
                            </xs:documentation>
                        </xs:annotation>
                        <xs:simpleType>
                            <xs:union memberTypes="StringLength1to16 OTA_CodeType"/>
                        </xs:simpleType>
                    </xs:attribute>
                    <xs:attribute name="MaxQuantity" type="StringLength1to16" use="optional">
                        <xs:annotation>
                            <xs:documentation xml:lang="en">Specifies the maximum quantity allowed for the option
                                identified by the CruiseOption attribute. Typically only used when the
                                ApplicationProfileTypeIndentifier contains the value "MAX".
                            </xs:documentation>
                        </xs:annotation>
                    </xs:attribute>
                </xs:complexType>
            </xs:element>
        </xs:sequence>
        <xs:attribute name="RuleId" type="StringLength1to16" use="optional">
            <xs:annotation>
                <xs:documentation xml:lang="en">Dining sitting - Specialty Inventory/ Dining Profile Identification
                </xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="RuleName" use="optional">
            <xs:annotation>
                <xs:documentation xml:lang="en">Identifies the cruise options for which Rules ( Special dining lilmits)
                    apply. i.e Category, Loyalty, Gratuity etc.
                </xs:documentation>
            </xs:annotation>
            <xs:simpleType>
                <xs:union memberTypes="StringLength1to16 OTA_CodeType"/>
            </xs:simpleType>
        </xs:attribute>
        <xs:attribute name="RuleLevel" use="optional">
            <xs:annotation>
                <xs:documentation xml:lang="en">Identifies the business hierarchy that the condition applies to.
                    Passenger, Booking, Group etc.
                </xs:documentation>
            </xs:annotation>
            <xs:simpleType>
                <xs:union memberTypes="StringLength1to16 OTA_CodeType"/>
            </xs:simpleType>
        </xs:attribute>
    </xs:complexType>
