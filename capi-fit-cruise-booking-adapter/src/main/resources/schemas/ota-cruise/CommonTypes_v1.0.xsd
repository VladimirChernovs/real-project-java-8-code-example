                    </xs:sequence>
                </xs:complexType>
            </xs:element>
            <xs:element name="TCCreditAmountInfos" minOccurs="0">
                <xs:complexType>
                    <xs:sequence>
                        <xs:element name="TCCreditAmounttInfo" type="PriceType" maxOccurs="unbounded"/>
                    </xs:sequence>
                </xs:complexType>
            </xs:element>
            <xs:element name="TCCDetailInfos" minOccurs="0">
                <xs:complexType>
                    <xs:sequence>
                        <xs:element name="TCCDetailInfo" type="TCCDetailInfoType" maxOccurs="unbounded"/>
                    </xs:sequence>
                </xs:complexType>
            </xs:element>
        </xs:sequence>
        <xs:attributeGroup ref="sitypes:CruiseGrpTourConductorCreditsGroup"/>
