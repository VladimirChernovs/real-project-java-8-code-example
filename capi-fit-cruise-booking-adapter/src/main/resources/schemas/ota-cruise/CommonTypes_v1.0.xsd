<?xml version="1.0" encoding="UTF-8"?>
<!-- edited with XMLSpy v2007 sp1 (http://www.altova.com) by RCL (Royal Caribbean Cruises Ltd.) -->
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns="http://services.rccl.com/rsvr/api/commontypes/v1.0"
           xmlns:sitypes="http://services.rccl.com/rsvr/api/simpletypes/v1.0"
           targetNamespace="http://services.rccl.com/rsvr/api/commontypes/v1.0" elementFormDefault="unqualified"
           attributeFormDefault="unqualified" version="1.0">
    <xs:import namespace="http://services.rccl.com/rsvr/api/simpletypes/v1.0" schemaLocation="SimpleTypes_v1.0.xsd"/>
    <xs:complexType name="SuccessType">
        <xs:annotation>
            <xs:documentation xml:lang="en">Standard way to indicate successful processing of an OTA
                message. Returning an empty element of this type indicates
                success.
            </xs:documentation>
        </xs:annotation>
    </xs:complexType>
    <xs:complexType name="WarningType">
        <xs:simpleContent>
            <xs:extension base="FreeTextType">
                <xs:attribute name="Type" type="sitypes:LookupCodeType" use="required">
                    <xs:annotation>
                        <xs:documentation xml:lang="en">The Warning element MUST contain the Type
                            attribute that uses a recommended set of values to indicate the warning
                            type. The validating XSD can expect to accept values that it has NOT
                            been explicitly coded for and process them by using Type ="Unknown".
                            Refer to OTA Code List Error Warning Type (EWT).
                        </xs:documentation>
                    </xs:annotation>
                </xs:attribute>
                <xs:attributeGroup ref="sitypes:ErrorWarningAttributeGroup"/>
            </xs:extension>
        </xs:simpleContent>
    </xs:complexType>
    <xs:complexType name="WarningsType">
        <xs:sequence>
            <xs:element name="Warning" minOccurs="0" maxOccurs="99">
                <xs:complexType>
                    <xs:simpleContent>
                        <xs:extension base="WarningType"/>
                    </xs:simpleContent>
                </xs:complexType>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="POS_Type">
        <xs:annotation>
            <xs:documentation xml:lang="en">Point of Sale (POS) is the details identifying the party
                or connection channel making the request.
            </xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="Source" type="SourceType" maxOccurs="10">
                <xs:annotation>
                    <xs:documentation xml:lang="en">This holds details regarding the requestor. It
                        may be repeated to also accommodate the delivery systems.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="SourceType">
        <xs:annotation>
            <xs:documentation xml:lang="en">Provides information on the source of a
                request.
            </xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="RequestorID" type="UniqueID_Type" minOccurs="0"/>
            <xs:element name="BookingChannel" minOccurs="0">
                <xs:annotation>
                    <xs:documentation xml:lang="en">Specifies the booking channel type and whether
                        it is the primary means of connectivity of the source.
                    </xs:documentation>
                </xs:annotation>
                <xs:complexType>
                    <xs:sequence>
                        <xs:element name="CompanyName" type="CompanyNameType" minOccurs="0">
                            <xs:annotation>
                                <xs:documentation xml:lang="en">Identifies the company that is
                                    associated with the booking channel.
                                </xs:documentation>
                            </xs:annotation>
                        </xs:element>
                    </xs:sequence>
                    <xs:attributeGroup ref="sitypes:BookingChannelGroup"/>
                </xs:complexType>
            </xs:element>
        </xs:sequence>
        <xs:attribute name="AgentSine" type="sitypes:StringLength1to16" use="optional">
            <xs:annotation>
                <xs:documentation xml:lang="en">Identifies the party within the requesting
                    entity.
                </xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="PseudoCityCode" type="sitypes:StringLength1to16" use="optional">
            <xs:annotation>
                <xs:documentation xml:lang="en">An identification code assigned to an office/agency
                    by a reservation system.
                </xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="ISOCountry" type="sitypes:ISO3166" use="optional">
            <xs:annotation>
                <xs:documentation xml:lang="en">The country code of the requesting
                    party.
                </xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="ISOCurrency" type="sitypes:AlphaLength3" use="optional">
            <xs:annotation>
                <xs:documentation xml:lang="en">The currency code in which the reservation will be
                    ticketed.
                </xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="AgentDutyCode" type="sitypes:StringLength1to16" use="optional">
            <xs:annotation>
                <xs:documentation xml:lang="en">An authority code assigned to a
                    requestor.
                </xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="AirlineVendorID" type="sitypes:UpperCaseAlphaNumericLength2to3" use="optional">
            <xs:annotation>
                <xs:documentation xml:lang="en">The IATA assigned airline code.</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="AirportCode" type="sitypes:UpperCaseAlphaNumericLength3to5" use="optional">
            <xs:annotation>
                <xs:documentation xml:lang="en">The IATA assigned airport code.</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="FirstDepartPoint" type="sitypes:StringLength3" use="optional">
            <xs:annotation>
                <xs:documentation xml:lang="en">The point of first departure in a
                    trip.
                </xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="ERSP_UserID" type="sitypes:StringLength1to16" use="optional">
            <xs:annotation>
                <xs:documentation xml:lang="en">Electronic Reservation Service Provider (ERSP)
                    assigned identifier used to identify the individual using the ERSP
                    system.
                </xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="TerminalID" type="sitypes:StringLength1to32" use="optional">
            <xs:annotation>
                <xs:documentation xml:lang="en">This is the electronic address of the device from
                    which information is entered.
                </xs:documentation>
            </xs:annotation>
        </xs:attribute>
    </xs:complexType>
    <xs:complexType name="CompanyNameType">
        <xs:annotation>
            <xs:documentation xml:lang="en">Identifies a company by name.</xs:documentation>
        </xs:annotation>
        <xs:simpleContent>
            <xs:extension base="sitypes:StringLength0to128">
                <xs:attributeGroup ref="sitypes:CompanyID_AttributesGroup"/>
                <xs:attribute name="Division" type="sitypes:StringLength1to32" use="optional">
                    <xs:annotation>
                        <xs:documentation xml:lang="en">The division name or ID with which the
                            contact is associated.
                        </xs:documentation>
                    </xs:annotation>
                </xs:attribute>
                <xs:attribute name="Department" type="sitypes:StringLength1to32" use="optional">
                    <xs:annotation>
                        <xs:documentation xml:lang="en">The department name or ID with which the
                            contact is associated.
                        </xs:documentation>
                    </xs:annotation>
                </xs:attribute>
                <xs:attributeGroup ref="sitypes:CompanyProfile_AttributesGroup"/>
            </xs:extension>
        </xs:simpleContent>
    </xs:complexType>
    <xs:complexType name="CompanyInfoType">
        <xs:annotation>
            <xs:documentation xml:lang="en">Identifies a company by profile.</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="CompanyName" type="CompanyNameType" minOccurs="0" maxOccurs="unbounded"/>
            <xs:element name="ContactInfo" type="ContactType" minOccurs="0" maxOccurs="unbounded"/>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="RestrictionInfoType">
        <xs:annotation>
            <xs:documentation xml:lang="en">profile restrictions</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="RestrictionInfo" type="GenericLineItemType" minOccurs="0" maxOccurs="unbounded"/>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="QualifierInfoType">
        <xs:annotation>
            <xs:documentation xml:lang="en">profile Qualifiers</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="Qualifier" type="GenericEchoTokenType" minOccurs="0" maxOccurs="unbounded"/>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="UniqueID_Type">
        <xs:sequence>
            <xs:element name="CompanyName" type="CompanyNameType" minOccurs="0"/>
        </xs:sequence>
        <xs:attributeGroup ref="sitypes:UniqueID_Group"/>
    </xs:complexType>
    <xs:complexType name="SailingInfoType">
        <xs:sequence>
            <xs:element name="SelectedSailing" minOccurs="0">
                <xs:complexType>
                    <xs:complexContent>
                        <xs:extension base="SailingBaseType">
                            <xs:attribute name="VoyageID" type="sitypes:StringLength1to32" use="optional"/>
                            <xs:attribute name="Status" type="sitypes:LookupCodeType" use="optional"/>
                            <xs:attribute name="SailingDescriptionCode" type="sitypes:ListOfOTA_CodeType"/>
                            <xs:attributeGroup ref="sitypes:DateSpanGroup"/>
                        </xs:extension>
                    </xs:complexContent>
                </xs:complexType>
            </xs:element>
            <xs:element name="InclusivePackageOption" minOccurs="0">
                <xs:complexType>
                    <xs:attributeGroup ref="sitypes:CruisePackageGroup"/>
                </xs:complexType>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="SailingBaseType">
        <xs:sequence>
            <xs:element name="CruiseLine">
                <xs:complexType>
                    <xs:attributeGroup ref="sitypes:ShipReferenceGroup"/>
                </xs:complexType>
            </xs:element>
            <xs:element name="Region" minOccurs="0">
                <xs:complexType>
                    <xs:attributeGroup ref="sitypes:RegionGroup"/>
                </xs:complexType>
            </xs:element>
            <xs:element name="DeparturePort" minOccurs="0">
                <xs:complexType>
                    <xs:simpleContent>
                        <xs:extension base="LocationType">
                            <xs:attribute name="EmbarkationTime" type="xs:dateTime" use="optional"/>
                        </xs:extension>
                    </xs:simpleContent>
                </xs:complexType>
            </xs:element>
            <xs:element name="ArrivalPort" minOccurs="0">
                <xs:complexType>
                    <xs:simpleContent>
                        <xs:extension base="LocationType">
                            <xs:attribute name="DebarkationDateTime" type="xs:dateTime"/>
                        </xs:extension>
                    </xs:simpleContent>
                </xs:complexType>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="GrpSailingType">
        <xs:complexContent>
            <xs:extension base="SailingInfoType">
                <xs:attribute name="MaxCabinOccupancy" type="sitypes:Numeric1to99" use="optional"/>
                <xs:attribute name="GAPPoints" type="sitypes:Numeric0to999" use="optional"/>
                <xs:attribute name="NCCFIncluded" type="sitypes:YesNoType" use="optional"/>
                <xs:attribute name="NextSailingDate" type="sitypes:DateStringType" use="optional"/>
                <xs:attribute name="PreviousSailingDate" type="sitypes:DateStringType" use="optional"/>
                <xs:attribute name="MinimumCabinsRequired" type="sitypes:Numeric1to99" use="optional"/>
                <xs:attribute name="MaximumCabinsAllowed" type="sitypes:Numeric1to99" use="optional"/>
                <xs:attribute name="TaxesAndFee" type="sitypes:Money" use="optional"/>
            </xs:extension>
            <!--<xs:sequence>
                <xs:element name="Dining" minOccurs="0" maxOccurs="10">
                <xs:complexType>
                <xs:attribute name="Sitting" type="sitypes:StringLength1to64" use="required"/>
                <xs:attribute name="Status" type="sitypes:LookupCodeType" use="optional"/>
                </xs:complexType>
                </xs:element>
                <xs:element name="Transportation" type="GuestTransportationType" minOccurs="0" maxOccurs="5"/>
                <xs:element name="Information" type="ParagraphType" minOccurs="0" maxOccurs="5"/>
                <xs:element name="GroupPolicyInfo" minOccurs="0" maxOccurs="10">
                <xs:complexType>
                <xs:attributeGroup ref="GroupPolicyTypeGroup"/>
                </xs:complexType>
                </xs:element>
                <xs:element name="CategoryOption" type="CategoryOptionType" maxOccurs="6"/>
                </xs:sequence>-->
            <!--<xs:attribute name="CategoryLocation" type="CategoryLocationType" use="optional"/>-->
        </xs:complexContent>
    </xs:complexType>
    <xs:complexType name="LocationType">
        <xs:annotation>
            <xs:documentation xml:lang="en">Code and optional string to describe a location
                point.
            </xs:documentation>
        </xs:annotation>
        <xs:simpleContent>
            <xs:extension base="xs:string">
                <xs:attributeGroup ref="sitypes:LocationGroup"/>
            </xs:extension>
        </xs:simpleContent>
    </xs:complexType>
    <xs:complexType name="RelatedTravelerType">
        <xs:annotation>
            <xs:documentation source="Description" xml:lang="en">Other traveler profiles associated
                with an specific individual.
            </xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="UniqueID" type="UniqueID_Type" minOccurs="0"/>
            <xs:element name="PersonName" type="PersonNameType" minOccurs="0"/>
            <xs:element name="LinkedReference" type="GenericLineItemType" minOccurs="0" maxOccurs="10"/>
        </xs:sequence>
        <xs:attribute name="Relation" type="sitypes:StringLength1to32" use="optional"/>
        <xs:attributeGroup ref="sitypes:BirthDateGroup"/>
    </xs:complexType>
    <xs:complexType name="ReservationResultsType">
        <xs:annotation>
            <xs:documentation source="Description" xml:lang="en">Booking Summary.</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="SailingInfo" type="GrpSailingType" minOccurs="0"/>
            <xs:element name="UniqueID" type="UniqueID_Type" minOccurs="0"/>
            <xs:element name="PersonName" type="PersonNameType" minOccurs="0"/>
            <xs:element name="Warnings" type="WarningsType" minOccurs="0"/>
        </xs:sequence>
        <xs:attribute name="TransferStatus" type="sitypes:LookupCodeType" use="optional"/>
        <xs:attribute name="ReservationStatus" type="sitypes:LookupCodeType" use="optional"/>
        <xs:attribute name="PaymentAllocationRequired" type="sitypes:LookupCodeType"/>
    </xs:complexType>
    <xs:complexType name="PersonNameType">
        <xs:sequence>
            <xs:element name="NamePrefix" type="sitypes:StringLength1to16" minOccurs="0" maxOccurs="3"/>
            <xs:element name="GivenName" type="sitypes:StringLength1to64" minOccurs="0" maxOccurs="5"/>
            <xs:element name="MiddleName" type="sitypes:StringLength1to64" minOccurs="0" maxOccurs="3"/>
            <xs:element name="Surname" type="sitypes:StringLength1to64"/>
            <xs:element name="NameSuffix" type="sitypes:StringLength1to16" minOccurs="0" maxOccurs="3"/>
            <xs:element name="Document" minOccurs="0">
                <xs:complexType>
                    <xs:attribute name="DocID" type="sitypes:StringLength1to32" use="optional"/>
                    <xs:attribute name="DocType" type="sitypes:LookupCodeType" use="optional"/>
                </xs:complexType>
            </xs:element>
        </xs:sequence>
        <xs:attribute name="Language" type="xs:language"/>
        <xs:attribute name="DefaultInd" type="xs:boolean"/>
    </xs:complexType>
    <xs:complexType name="AddressType">
        <xs:sequence>
            <xs:element name="AddressLine" type="sitypes:StringLength1to255" minOccurs="0" maxOccurs="5"/>
            <xs:element name="CityName" type="sitypes:StringLength1to64" minOccurs="0"/>
            <xs:element name="PostalCode" type="sitypes:StringLength1to16" minOccurs="0"/>
            <xs:element name="County" type="sitypes:StringLength1to32" minOccurs="0"/>
            <xs:element name="StateProv" type="sitypes:StringLength0to64" minOccurs="0"/>
            <xs:element name="Province" type="sitypes:StringLength0to64" minOccurs="0"/>
            <xs:element name="CountryName" type="CountryType" minOccurs="0"/>
        </xs:sequence>
        <xs:attribute name="Type" type="sitypes:StringLength0to32" use="optional"/>
        <xs:attribute name="Remark" type="sitypes:StringLength1to128" use="optional"/>
        <xs:attribute name="Language" type="xs:language" use="optional"/>
    </xs:complexType>
    <xs:complexType name="EmailType">
        <xs:simpleContent>
            <xs:extension base="sitypes:StringLength1to255">
                <xs:attribute name="DefaultInd" type="xs:boolean" use="optional"/>
                <xs:attribute name="EmailType" type="sitypes:LookupCodeType" use="optional"/>
                <xs:attribute name="Remark" type="sitypes:StringLength1to128" use="optional"/>
            </xs:extension>
        </xs:simpleContent>
    </xs:complexType>
    <xs:complexType name="CountryType">
        <xs:simpleContent>
            <xs:extension base="sitypes:StringLength0to64">
                <xs:attribute name="Code" type="sitypes:ISO3166" use="optional"/>
            </xs:extension>
        </xs:simpleContent>
    </xs:complexType>
    <xs:complexType name="ContactType">
        <xs:sequence>
            <xs:element name="PersonName" type="PersonNameType" minOccurs="0"/>
            <xs:element name="Telephone" minOccurs="0" maxOccurs="5">
                <xs:complexType>
                    <xs:attributeGroup ref="sitypes:TelephoneAttributesGroup"/>
                </xs:complexType>
            </xs:element>
            <xs:element name="Address" type="AddressType" minOccurs="0" maxOccurs="5"/>
            <xs:element name="Email" type="EmailType" minOccurs="0" maxOccurs="5"/>
        </xs:sequence>
        <xs:attribute name="DefaultInd" type="xs:boolean" use="optional"/>
        <xs:attribute name="ContactType" type="sitypes:StringLength1to32" use="optional"/>
        <xs:attribute name="Emergency" type="xs:boolean" use="optional"/>
        <xs:attribute name="RPH" type="sitypes:StringLength1to255" use="optional"/>
        <xs:attributeGroup ref="sitypes:BirthDateGroup"/>
    </xs:complexType>
    <xs:complexType name="GuestBaseType">
        <xs:sequence>
            <xs:element name="ContactInfo" type="PersonNameType" minOccurs="0"/>
            <xs:element name="Address" type="AddressType" minOccurs="0"/>
            <xs:element name="Telephone" minOccurs="0" maxOccurs="3">
                <xs:complexType>
                    <xs:attributeGroup ref="sitypes:TelephoneAttributesGroup"/>
                </xs:complexType>
            </xs:element>
            <xs:element name="Email" type="EmailType" minOccurs="0" maxOccurs="3"/>
            <xs:element name="Transportation" type="GuestTransportationType" minOccurs="0"/>
            <xs:element name="TravelDocuments" type="DocumentType" minOccurs="0"/>
        </xs:sequence>
        <xs:attributeGroup ref="sitypes:GuestInfoGroup"/>
    </xs:complexType>
    <xs:complexType name="GuestQualifierType">
        <xs:complexContent>
            <xs:extension base="GuestBaseType">
                <xs:sequence>
                    <xs:element name="EligibilityQualifier" type="GenericLineItemType" minOccurs="0" maxOccurs="99"/>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
    <xs:complexType name="GenericLineItemType">
        <xs:attribute name="Name" type="sitypes:StringLength0to255"/>
        <xs:attribute name="Value" type="sitypes:StringLength0to255"/>
        <xs:attribute name="Type" type="sitypes:StringLength0to255"/>
    </xs:complexType>
    <xs:complexType name="GenericEchoTokenType">
        <xs:attribute name="Name" type="sitypes:StringLength0to255"/>
        <xs:attribute name="Value" type="sitypes:StringLength0to255"/>
        <xs:attribute name="Type" type="sitypes:StringLength0to255"/>
        <xs:attribute name="Code" type="sitypes:StringLength0to255"/>
        <xs:attribute name="RPH" type="sitypes:StringLength0to255"/>
    </xs:complexType>
    <xs:complexType name="RateQualifierItemType">
        <xs:complexContent>
            <xs:extension base="GenericLineItemType">
                <xs:sequence>
                    <xs:element name="Code" minOccurs="0">
                        <xs:complexType>
                            <xs:sequence>
                                <xs:element name="Item" type="GenericLineItemType" minOccurs="0" maxOccurs="99"/>
                            </xs:sequence>
                        </xs:complexType>
                    </xs:element>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
    <xs:complexType name="CabinPolicyType"/>
    <xs:complexType name="PolicyInfoType">
        <xs:sequence>
            <xs:element name="PolicyInfo" maxOccurs="10">
                <xs:complexType>
                    <xs:attributeGroup ref="sitypes:PolicyTypeGroup"/>
                </xs:complexType>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="ProfileInfoType">
        <xs:sequence>
            <xs:element name="CompanyInfo" type="CompanyInfoType" minOccurs="0"/>
            <xs:element name="Qualifiers" type="QualifierInfoType" minOccurs="0"/>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="GuestTransportationType">
        <xs:sequence>
            <xs:element name="GuestCity" type="LocationType" minOccurs="0"/>
            <xs:element name="GatewayCity" type="LocationType" minOccurs="0"/>
        </xs:sequence>
        <xs:attribute name="Mode" type="sitypes:LookupCodeType" use="optional"/>
        <xs:attribute name="Status" type="sitypes:LookupCodeType" use="optional"/>
    </xs:complexType>
    <xs:complexType name="DocumentType">
        <xs:sequence>
            <xs:element name="DocHolderName" type="sitypes:StringLength1to64" minOccurs="0"/>
            <xs:element name="DocLimitations" type="sitypes:StringLength1to64" minOccurs="0" maxOccurs="9">
                <xs:annotation>
                    <xs:documentation source="Description" xml:lang="en">Used to indicate any
                        limitations on the document (e.g. as a person may only be allowed to spend a
                        max of 30 days in country on a visitor's visa).
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
        <xs:attribute name="DocIssueAuthority" type="sitypes:StringLength1to64" use="optional"/>
        <xs:attribute name="DocIssueLocation" type="sitypes:StringLength1to64" use="optional"/>
        <xs:attribute name="DocID" type="sitypes:StringLength1to32" use="optional"/>
        <xs:attribute name="DocType" type="sitypes:LookupCodeType" use="optional"/>
        <xs:attributeGroup ref="sitypes:EffectiveExpireDateGroup"/>
        <xs:attribute name="DocIssueCountry" type="sitypes:ISO3166" use="optional"/>
        <xs:attribute name="BirthCountry" type="sitypes:ISO3166" use="optional"/>
        <xs:attribute name="BirthPlace" type="sitypes:StringLength1to64" use="optional"/>
        <xs:attribute name="DocHolderNationality" type="sitypes:ISO3166" use="optional"/>
        <xs:attribute name="ContactName" type="sitypes:StringLength1to64" use="optional"/>
        <xs:attribute name="Remark" type="sitypes:StringLength1to128" use="optional"/>
        <xs:attribute name="PostalCode" type="sitypes:StringLength1to16" use="optional"/>
    </xs:complexType>
    <xs:complexType name="GrpProfileInfoType">
        <xs:sequence>
            <xs:element name="GroupID" type="UniqueID_Type" minOccurs="0"/>
        </xs:sequence>
        <xs:attribute name="GroupName" type="sitypes:StringLength0to255" use="optional"/>
        <xs:attribute name="GroupType" type="sitypes:LookupCodeType" use="optional"/>
        <xs:attribute name="GroupSubType" type="sitypes:LookupCodeType" use="optional">
            <xs:annotation>
                <xs:documentation>Provides the type of Group. ( e.g Affinity / Promotional
                    )
                </xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="GroupStatus" type="sitypes:LookupCodeType" use="optional">
            <xs:annotation>
                <xs:documentation>Status of the Group. (Offered / Booked / Cancelled etc. (Ignored
                    in the request messages.) OTACodeType
                </xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="TaxesAndFee" type="sitypes:Money" use="optional"/>
        <xs:attribute name="GroupTypeText" type="sitypes:StringLength1to255" use="optional"/>
        <xs:attribute name="GroupProfileDescription" type="sitypes:StringLength1to255" use="optional"/>
        <xs:attribute name="GroupGuestCount" type="sitypes:Numeric0to999" use="optional"/>
        <xs:attribute name="GroupEffectiveDate" type="sitypes:DateOrTimeOrDateTimeType" use="optional"/>
        <xs:attribute name="GroupBookDate" type="sitypes:DateOrTimeOrDateTimeType" use="optional"/>
        <xs:attribute name="AvailableGAPPoints" type="sitypes:Numeric0to999"/>
        <xs:attribute name="AllocatedGapPoints" type="sitypes:Numeric0to999"/>
        <xs:attribute name="OriginalGAPPoints" type="sitypes:Numeric0to999" use="optional">
            <xs:annotation>
                <xs:documentation>Provides redemption points for group amenities (Ignored in the
                    request messages).
                </xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="VendorGroupCoordinator" type="sitypes:StringLength1to64" use="optional">
            <xs:annotation>
                <xs:documentation>Provides First Name of Group Coordinator from the Vendor company.
                    e.g RCCL Group Coordinator.
                </xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="VendorGroupAirCoordinator" type="sitypes:StringLength1to64" use="optional">
            <xs:annotation>
                <xs:documentation>Provides First Name of Group AIR Coordinator from the Vendor
                    company. e.g RCCL Group Coordinator.
                </xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="VacationProtection" type="sitypes:YesNoType" use="optional">
            <xs:annotation>
                <xs:documentation>Identifies whether a Group is covered by Vacation Protection
                    Program or not.
                </xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="ContractGroup" type="sitypes:YesNoType" use="optional"/>
        <xs:attribute name="ContractDate" type="xs:date" use="optional"/>
        <xs:attribute name="ContractType" type="sitypes:LookupCodeType"/>
        <xs:attribute name="AmenitiesExists" type="sitypes:YesNoType" use="optional"/>
        <xs:attribute name="DiningConfiguration" use="optional">
            <xs:annotation>
                <xs:documentation>Indicates Dining configuration for the whole group. Nametoken.
                </xs:documentation>
            </xs:annotation>
            <xs:simpleType>
                <xs:restriction base="xs:NMTOKEN">
                    <xs:enumeration value="Apart"/>
                    <xs:enumeration value="Together"/>
                    <xs:enumeration value="RoundRobin"/>
                    <xs:enumeration value="DoNotSeat"/>
                </xs:restriction>
            </xs:simpleType>
        </xs:attribute>
        <xs:attribute name="PrePaidGratuities" type="sitypes:YesNoType" use="optional"/>
        <xs:attribute name="ReinstatedGroup" type="sitypes:YesNoType" use="optional"/>
        <xs:attribute name="DocumentationLanguage" type="xs:language" use="optional"/>
        <xs:attribute name="ReleaseTickets" type="sitypes:YesNoType" use="optional"/>
        <xs:attribute name="HoldTickets" type="sitypes:YesNoType" use="optional"/>
        <xs:attribute name="ExtendOption" type="sitypes:YesNoType" use="optional">
            <xs:annotation>
                <xs:documentation>Makes a request to extend the payment option dates for a
                    group.
                </xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="InternationalGroup" type="sitypes:YesNoType" use="optional"/>
        <xs:attribute name="GroupDirectContactPassengerID" type="sitypes:StringLength1to32" use="optional">
            <xs:annotation>
                <xs:documentation>Identifies the ID of the passenger thats designated as the direct
                    contact person for the group
                </xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attributeGroup ref="sitypes:GrpAuditGroup"/>
        <xs:attributeGroup ref="sitypes:CruiseGrpTourConductorCreditsGroup"/>
        <xs:attribute name="GroupPolicyType" type="sitypes:StringLength0to32" use="optional">
            <xs:annotation>
                <xs:documentation>Identifies the group payment policy type</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="UnAssignedDiningQuantity" type="sitypes:Numeric0to9999" use="optional">
            <xs:annotation>
                <xs:documentation>Provides unassigned count facility for Promotional Groups etc
                    where Allocation is not allowed until Bookings are created within the
                    Group
                </xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="MoreGroupsExist" type="xs:boolean"/>
    </xs:complexType>
    <xs:complexType name="FreeTextType">
        <xs:simpleContent>
            <xs:extension base="xs:string">
                <xs:attributeGroup ref="sitypes:LanguageGroup"/>
            </xs:extension>
        </xs:simpleContent>
    </xs:complexType>
    <xs:complexType name="CabinOptionType">
        <xs:annotation>
            <xs:documentation xml:lang="en">Specifies cabin option info.</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="CabinConfiguration" minOccurs="0" maxOccurs="9">
                <xs:annotation>
                    <xs:documentation xml:lang="en">Specifies cabin bed
                        configuration.
                    </xs:documentation>
                </xs:annotation>
                <xs:complexType>
                    <xs:attribute name="BedConfigurationCode" type="sitypes:StringLength1to8" use="optional">
                        <xs:annotation>
                            <xs:documentation xml:lang="en">Contains a code defining the bed
                                configuration in the cabin.
                            </xs:documentation>
                        </xs:annotation>
                    </xs:attribute>
                    <xs:attribute name="ViewObstruction" type="sitypes:Numeric0to999" use="optional">
                        <xs:annotation>
                            <xs:documentation xml:lang="en">This value gives the obstructed view percentage for the
                                cabin window
                            </xs:documentation>
                        </xs:annotation>
                    </xs:attribute>
                </xs:complexType>
            </xs:element>
            <xs:element name="MeasurementInfo" minOccurs="0" maxOccurs="9">
                <xs:annotation>
                    <xs:documentation xml:lang="en">Provides area information related to the
                        cabin.
                    </xs:documentation>
                </xs:annotation>
                <xs:complexType>
                    <xs:attributeGroup ref="sitypes:UnitsOfMeasureGroup">
                        <xs:annotation>
                            <xs:documentation xml:lang="en">Provides the unit of measure information
                                for the item identified by the Code.
                            </xs:documentation>
                        </xs:annotation>
                    </xs:attributeGroup>
                    <xs:attributeGroup ref="sitypes:OptionalCodeOptionalNameGroup">
                        <xs:annotation>
                            <xs:documentation xml:lang="en">This will provide the item for which
                                measurements are being provided (e.g., balcony,
                                cabin).
                            </xs:documentation>
                        </xs:annotation>
                    </xs:attributeGroup>
                    <xs:attribute name="DimensionInfo" type="sitypes:StringLength1to32" use="optional">
                        <xs:annotation>
                            <xs:documentation xml:lang="en">The dimensions associated with the
                                cabin, balcony, or window (e.g., 10x12)
                            </xs:documentation>
                        </xs:annotation>
                    </xs:attribute>
                </xs:complexType>
            </xs:element>
            <xs:element name="Remark" type="FreeTextType" minOccurs="0">
                <xs:annotation>
                    <xs:documentation xml:lang="en">Remark information related to the cabin
                        option.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
        <xs:attribute name="Status" type="sitypes:LookupCodeType" use="required">
            <xs:annotation>
                <xs:documentation xml:lang="en">Specifies the status of the cabin. Refer to OTA Code
                    table Status (STS).
                </xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="CategoryLocation" type="sitypes:CategoryLocationType" use="optional">
            <xs:annotation>
                <xs:documentation xml:lang="en">Specifies the category location.</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="ShipSide" use="optional">
            <xs:annotation>
                <xs:documentation xml:lang="en">Identifies the ship side location of the
                    cabin.
                </xs:documentation>
            </xs:annotation>
            <xs:simpleType>
                <xs:restriction base="xs:NMTOKEN">
                    <xs:enumeration value="Starboard">
                        <xs:annotation>
                            <xs:documentation xml:lang="en">Right side.</xs:documentation>
                        </xs:annotation>
                    </xs:enumeration>
                    <xs:enumeration value="Port">
                        <xs:annotation>
                            <xs:documentation xml:lang="en">Left side.</xs:documentation>
                        </xs:annotation>
                    </xs:enumeration>
                </xs:restriction>
            </xs:simpleType>
        </xs:attribute>
        <xs:attribute name="PositionInShip" use="optional">
            <xs:annotation>
                <xs:documentation xml:lang="en">Position of the cabin in ship (e.g.,
                    aft).
                </xs:documentation>
            </xs:annotation>
            <xs:simpleType>
                <xs:restriction base="xs:NMTOKEN">
                    <xs:enumeration value="Aft">
                        <xs:annotation>
                            <xs:documentation xml:lang="en">This is the back of the
                                ship.
                            </xs:documentation>
                        </xs:annotation>
                    </xs:enumeration>
                    <xs:enumeration value="Forward">
                        <xs:annotation>
                            <xs:documentation xml:lang="en">This is the front of the
                                ship.
                            </xs:documentation>
                        </xs:annotation>
                    </xs:enumeration>
                    <xs:enumeration value="Midship">
                        <xs:annotation>
                            <xs:documentation xml:lang="en">This is the middle of the
                                ship.
                            </xs:documentation>
                        </xs:annotation>
                    </xs:enumeration>
                </xs:restriction>
            </xs:simpleType>
        </xs:attribute>
        <xs:attribute name="BedType" type="sitypes:LookupCodeType" use="optional">
            <xs:annotation>
                <xs:documentation xml:lang="en">Specifies the bed type for this cabin. Refer to OTA
                    Codelist Bed Type (BED).
                </xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attributeGroup ref="sitypes:CruiseCabinGroup"/>
        <xs:attribute name="ReleaseDateTime" type="xs:dateTime" use="optional">
            <xs:annotation>
                <xs:documentation xml:lang="en">The date and time the held cabin will be released
                    back to inventory if not booked. Typically, this is the same ReleaseDateTime
                    that was provided in the CabinHoldRS.
                </xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attributeGroup ref="sitypes:DeckGroup">
            <xs:annotation>
                <xs:documentation xml:lang="en">Provides the deck name and
                    number.
                </xs:documentation>
            </xs:annotation>
        </xs:attributeGroup>
        <xs:attribute name="CabinCategoryStatusCode" type="sitypes:LookupCodeType" use="optional">
            <xs:annotation>
                <xs:documentation xml:lang="en">The status of the berth category of the cabin. Refer
                    to OTA Code Table STS (Status).
                </xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="CabinCategoryCode" type="sitypes:StringLength1to8" use="optional">
            <xs:annotation>
                <xs:documentation xml:lang="en">The berth category of the cabin.</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="CabinRanking" type="sitypes:Numeric1to999" use="optional">
            <xs:annotation>
                <xs:documentation xml:lang="en">The relative ranking of the cabin based on location
                    preference, a better view, etc.
                </xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="ConnectingCabinIndicator" type="xs:boolean" use="optional">
            <xs:annotation>
                <xs:documentation xml:lang="en">If true, the cabin is connected to another
                    cabin.
                </xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="ConnectingCabinNumber" type="sitypes:StringLength1to8" use="optional">
            <xs:annotation>
                <xs:documentation xml:lang="en">The number of the connecting
                    cabin.
                </xs:documentation>
            </xs:annotation>
        </xs:attribute>
    </xs:complexType>
    <xs:complexType name="TPA_PromotionInfoType">
        <xs:attributeGroup ref="sitypes:PromotionCodeGroup"/>
    </xs:complexType>
    <xs:complexType name="TCCSummaryInfoType">
        <xs:sequence>
            <xs:element name="Profile" type="ProfileInfoType" minOccurs="0"/>
            <xs:sequence>
                <xs:element name="TCCUsageSummaryInfo" type="TCCUsageSummaryInfoType" minOccurs="0"/>
            </xs:sequence>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="TCCUsageSummaryInfoType">
        <xs:sequence>
            <xs:element name="TCCreditCountInfos" minOccurs="0">
