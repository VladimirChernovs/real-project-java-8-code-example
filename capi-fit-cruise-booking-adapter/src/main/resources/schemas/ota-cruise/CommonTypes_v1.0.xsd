    <xs:complexType name="TCCDetailInfoType">
        <xs:sequence>
            <xs:element name="TCCreditCountInfos" minOccurs="0">
                <xs:complexType>
                    <xs:sequence>
                        <xs:element name="TCCreditCountInfo" type="CabinCountsType" maxOccurs="unbounded"/>
                    </xs:sequence>
                </xs:complexType>
            </xs:element>
            <xs:element name="TCCreditAmountInfos" minOccurs="0">
                <xs:complexType>
                    <xs:sequence>
                        <xs:element name="TCCreditAmounttInfo" type="PriceType" maxOccurs="unbounded"/>
                    </xs:sequence>
                </xs:complexType>
            </xs:element>
        </xs:sequence>
        <xs:attribute name="Code" type="sitypes:StringLength0to32"/>
        <xs:attribute name="Type" type="sitypes:LookupCodeType"/>
        <xs:attribute name="OverrideFlag" type="sitypes:YesNoType"/>
        <xs:attribute name="RequestSequence" type="sitypes:Numeric0to999"/>
        <xs:attribute name="CabinCategoryCode" type="sitypes:StringLength1to8" use="optional"/>
        <xs:attribute name="PriceIdDescription" type="sitypes:StringLength0to32"/>
    </xs:complexType>
    <xs:complexType name="SelectedCategoryType">
        <xs:annotation>
            <xs:documentation>Specifies Selected category information for a reservation, includes
                cabin selection and cabin attribute details
            </xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="Warnings" type="WarningsType" minOccurs="0"/>
            <xs:element name="SelectedPromotions" type="TPA_PromotionInfoType" minOccurs="0" maxOccurs="25"/>
            <xs:element name="CabinAttributes" minOccurs="0">
                <xs:annotation>
                    <xs:documentation xml:lang="en">Collection of category-level cabin
                        attributes.
                    </xs:documentation>
                </xs:annotation>
                <xs:complexType>
                    <xs:sequence>
                        <xs:element name="CabinAttribute" maxOccurs="99">
                            <xs:annotation>
                                <xs:documentation>List of attributes associated with all cabins in
                                    the specified category.
                                </xs:documentation>
                            </xs:annotation>
                            <xs:complexType>
                                <xs:attribute name="CabinAttributeCode" type="sitypes:LookupCodeType" use="optional">
                                    <xs:annotation>
                                        <xs:documentation xml:lang="en">Specifies the type of cabin
                                            requested. Refer to OTA Code List Cabin Filters
                                            (CBF).
                                        </xs:documentation>
                                    </xs:annotation>
                                </xs:attribute>
                            </xs:complexType>
                        </xs:element>
                    </xs:sequence>
                </xs:complexType>
            </xs:element>
            <xs:element name="CabinsInfos" minOccurs="0">
                <xs:complexType>
                    <xs:sequence>
                        <xs:element name="CabinInfo" minOccurs="0" maxOccurs="200">
                            <xs:complexType>
                                <xs:complexContent>
                                    <xs:extension base="CabinOptionType">
                                        <xs:sequence>
                                            <xs:element name="CabinAttributes" minOccurs="0">
                                                <xs:complexType>
                                                    <xs:sequence>
                                                        <xs:element name="CabinAttribute" maxOccurs="99">
                                                            <xs:complexType>
                                                                <xs:attribute name="CabinAttributeCode"
                                                                              type="sitypes:LookupCodeType"
                                                                              use="optional"/>
                                                            </xs:complexType>
                                                        </xs:element>
                                                    </xs:sequence>
                                                </xs:complexType>
                                            </xs:element>
                                        </xs:sequence>
                                        <xs:attribute name="RetainCabin" type="sitypes:YesNoType" use="optional"/>
                                        <xs:attribute name="CabinInventoryActionCode"
                                                      type="sitypes:AlphaNumericStringLength1" use="optional"/>
                                    </xs:extension>
                                </xs:complexContent>
                            </xs:complexType>
                        </xs:element>
                    </xs:sequence>
                </xs:complexType>
            </xs:element>
        </xs:sequence>
        <xs:attributeGroup ref="sitypes:CruiseCategoryDetailGroup">
            <xs:annotation>
                <xs:documentation xml:lang="en">Specificies cruise category information.
                </xs:documentation>
            </xs:annotation>
        </xs:attributeGroup>
        <xs:attribute name="SequenceNumber" type="sitypes:Numeric1to999">
            <xs:annotation>
                <xs:documentation>Indicates relative sequence number</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="HeldIndicator" type="sitypes:YesNoType"/>
        <xs:attribute name="WaitlistIndicator" type="xs:boolean" use="optional">
            <xs:annotation>
                <xs:documentation xml:lang="en">When true, this indicates the selected category is
                    waitlisted.
                </xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attributeGroup ref="sitypes:FareGroup"/>
        <xs:attribute name="UpgradeEligibility" type="sitypes:YesNoType"/>
        <xs:attribute name="CabinNumber" type="sitypes:NumericStringLength1to5"/>
        <xs:attribute name="ProximityReferenceCabin" type="sitypes:NumericStringLength1to5"/>
    </xs:complexType>
    <xs:complexType name="SelectedScheduledPaymentType">
        <xs:annotation>
            <xs:documentation>Specifies Selected category information for a reservation, includes
                cabin selection and cabin attribute details
            </xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="PaymentSchedule" type="PaymentScheduleCardType" minOccurs="0" maxOccurs="99"/>
        </xs:sequence>
        <xs:attribute name="SequenceNumber" type="sitypes:Numeric1to999">
            <xs:annotation>
                <xs:documentation>Indicates relative sequence number</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attributeGroup ref="sitypes:PaymentInstallmentScheduleAttributeGroup"/>
    </xs:complexType>
    <xs:complexType name="GrpCategoryAllocationInfoType">
        <xs:sequence>
            <xs:element name="Occupancy" type="sitypes:Numeric1to99"/>
            <xs:element name="SelectedFare">
                <xs:complexType>
                    <xs:attributeGroup ref="sitypes:FareGroup"/>
                    <xs:attributeGroup ref="sitypes:TPA_PromotionPriceGroup"/>
                </xs:complexType>
            </xs:element>
            <xs:element name="SelectedCategory" type="SelectedCategoryType">
                <xs:annotation>
                    <xs:documentation xml:lang="en">Specifies a selected cabin
                        category.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="InclusivePackageOption" minOccurs="0">
                <xs:annotation>
                    <xs:documentation xml:lang="en">Used to request package options that the cruise
                        might offer with the sailing. It is quite common to request, at this stage,
                        packages that are included in the cruise sailing offering (inclusive
                        packages).
                    </xs:documentation>
                </xs:annotation>
                <xs:complexType>
                    <xs:attributeGroup ref="sitypes:CruisePackageGroup"/>
                </xs:complexType>
            </xs:element>
        </xs:sequence>
        <xs:attribute name="RequestSequence" type="sitypes:Numeric0to999">
            <xs:annotation>
                <xs:documentation>Indicates relative sequence number</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="RequestedCabinsQty" type="sitypes:Numeric0to9999">
            <xs:annotation>
                <xs:documentation>Cabins Requested</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="NamedCabinGtysQty" type="sitypes:Numeric0to9999">
            <xs:annotation>
                <xs:documentation>Cabins Guaranteed with Names</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="UnNamedCabinGtysQty" type="sitypes:Numeric0to9999">
            <xs:annotation>
                <xs:documentation>Cabins Guaranteed without Names</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="NamedCabinsQty" type="sitypes:Numeric0to9999">
            <xs:annotation>
                <xs:documentation>Cabins Allocated with Names</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="UnNamedCabinsQty" type="sitypes:Numeric0to9999">
            <xs:annotation>
                <xs:documentation>Cabins Allocated without Names</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="WaitlistedCabinsQty" type="sitypes:Numeric0to9999">
            <xs:annotation>
                <xs:documentation>Cabins on Waitlist</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="ComplimentaryBerthsQty" type="sitypes:Numeric0to9999">
            <xs:annotation>
                <xs:documentation>Earned complimentary cabins - Tour Conductor Credit
                    equivalent.(TCC)
                </xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="SpecialFare" type="sitypes:YesNoType">
            <xs:annotation>
                <xs:documentation>Special Fare Flag at Group Inventory Level.</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="SpecialtyInventory" type="sitypes:YesNoType">
            <xs:annotation>
                <xs:documentation>Indicates specialty inventory for the category</xs:documentation>
            </xs:annotation>
        </xs:attribute>
    </xs:complexType>
    <xs:complexType name="CategorySearchQualifierType">
        <xs:sequence>
            <xs:element name="Occupancy" type="sitypes:Numeric1to99"/>
            <xs:element name="SelectedCategory" type="SelectedCategoryType"/>
